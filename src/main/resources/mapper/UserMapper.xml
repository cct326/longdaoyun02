<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cct.longdaoyun02.mapper.UserMapper">

    <select id="findByUsername" resultType="cct.longdaoyun02.entity.User" parameterType="string">
      select * from user
      where username = #{username}
    </select>
</mapper>